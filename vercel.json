{
  "version": 2,
  "name": "anastasia-site",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "installCommand": "npm install",
  "rewrites": [
    {
      "destination": "/index.html",
      "source": "/(.*)"
    }
  ]
}
